<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/more"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="@string/more_details"
        android:textSize="17sp" />

    <Button
        android:id="@+id/Test1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/more"
        android:layout_alignParentStart="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:text="@string/fuckits" />

    <Button
        android:id="@+id/Test2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/more"
        android:layout_marginStart="20sp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@id/Test1"
        android:text="@string/killits" />

    <Button
        android:id="@+id/decrypt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/more"
        android:layout_marginStart="20sp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@id/Test2"
        android:text="@string/decrypt" />

    <Button
        android:id="@+id/ctf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/more"
        android:layout_marginStart="20sp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@id/decrypt"
        android:text="@string/ctf" />


    <EditText
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ctf"
        android:layout_alignParentStart="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@null"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:maxLines="512"
        android:scrollbars="vertical"
        android:text="@string/app_name"
        android:textCursorDrawable="@null"
        android:textIsSelectable="true"
        android:textSize="17sp" />

    <FrameLayout
        android:id="@+id/frame_content"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_alignParentBottom="true">

    </FrameLayout>
    <!--<com.example.pwd61.analysis.views.PlasmaView-->
        <!--android:id="@+id/plasam"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="200dp"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--&gt;-->

    <!--</com.example.pwd61.analysis.views.PlasmaView>-->
</RelativeLayout>